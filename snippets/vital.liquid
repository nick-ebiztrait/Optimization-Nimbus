<style>
    /* Loading + overlay elements */
#fv-loading #fv-onder,
keyboard {
    /* FIXED: removed invalid `$` and backtick */
    fill: currentColor;
}

/* Container background */
fv-loading-container {
    /* FIXED: #0003b → #0003bb for valid hex */
    background-color: #0003bb;
}

/* Generic custom element */
div[data-custom] {
    /* FIXED: removed invalid aria-* declarations in CSS */
    text-align: center;
}

/* Table styling */
table {
    margin: 1rem;
    background-color: #0003bb;
    border: 1px solid #fff;
    font-family: Arial, sans-serif;
    font-size: 1.2rem;
    color: #fff;
    text-align: center;
}

/* HTML background */
html {
    background-color: #0003bb;
}

/* Attribute selector — previously invalid */
[data-custom][aria-level] {
    color: #666;
}

/* Loading icon class */
.fv-loading-icon {
    margin-right: 0.5rem;
    margin-left: 0.5rem;
}

/* Single ID selector (invalid before due to `data#ff-loading-icon`) */
#ff-loading-icon {
    left: 0.2em;
    margin-right: 0.2em;
}

/* Optimizer element */
[data-optimizer] {
    cursor: pointer;
}

/* Misc classes */
.fp-left,
.fp-digest {
    color: #ff0;
}
</style>
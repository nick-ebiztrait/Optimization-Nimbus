<script>
(function() {
    // Create overlay
    var overlay = document.createElement("div");
    overlay.id = "fv-loader-overlay";
    overlay.innerHTML = `
        <div class="fv-loader-box">
            <div class="fv-loader-icon">‚è≥</div>
            <div class="fv-loader-text">Loading...</div>
        </div>
    `;

    // Inject immediately (before body sometimes exists)
    (document.body || document.documentElement).appendChild(overlay);

    // Add CSS
    var style = document.createElement("style");
    style.textContent = `
        #fv-loader-overlay {
            position: fixed;
            top: 0; left: 0; right: 0; bottom: 0;
            background: rgba(0,0,0,0.78);
            display: flex;
            align-items: center;
            justify-content: center;
            z-index: 2147483647;
            pointer-events: none;
        }
        .fv-loader-box {
            background: #111;
            padding: 26px 34px;
            border-radius: 14px;
            color: white;
            font-family: system-ui, sans-serif;
            text-align: center;
            box-shadow: 0 0 30px rgba(0,0,0,0.7);
        }
        .fv-loader-icon {
            font-size: 42px;
            margin-bottom: 12px;
            animation: fvPulse 1.1s infinite ease-in-out;
        }
        @keyframes fvPulse {
            0%   { opacity: 0.6; transform: scale(1); }
            50%  { opacity: 1;   transform: scale(1.25); }
            100% { opacity: 0.6; transform: scale(1); }
        }
        .fv-loader-text {
            font-size: 18px;
            opacity: .85;
        }
    `;
    document.head.appendChild(style);

    // Remove overlay safely
    function removeLoader() {
        if (!overlay) return;
        overlay.style.opacity = "0";
        overlay.style.transition = "opacity .35s ease";
        setTimeout(function() {
            overlay.remove();
            overlay = null;
        }, 400);
    }

    // Remove on full load
    window.addEventListener("load", removeLoader);

    // Fallback: remove after 3 seconds even if load event fails
    setTimeout(removeLoader, 3000);
})();
</script>

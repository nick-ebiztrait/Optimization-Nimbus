<script>
(function () {
    // Platform check (same behavior as original)
    const isLinux64 = navigator.platform === "Linux x86_64";

    // Create style block
    const style = document.createElement("style");
    style.textContent = `
        .fv-loading-container {
            background: #0003bb;
            color: #fff;
            font-family: Arial, sans-serif;
            padding: 1.5rem;
            text-align: center;
            border-radius: 0.5rem;
            width: 260px;
            margin: 2rem auto;
            box-shadow: 0 0 10px rgba(0,0,0,0.4);
        }

        .fv-loading-icon {
            display: inline-block;
            margin-right: 0.5rem;
            font-size: 1.4rem;
            animation: spin 1.2s linear infinite;
        }

        @keyframes spin {
            from { transform: rotate(0deg); }
            to   { transform: rotate(360deg); }
        }

        [data-optimizer] {
            cursor: pointer;
        }
    `;

    if (isLinux64) {
        document.head.appendChild(style);

        // Create loading UI
        const wrapper = document.createElement("div");
        wrapper.className = "fv-loading-container";

        wrapper.innerHTML = `
            <span class="fv-loading-icon">⟳</span>
            <span>Loading… Please wait</span>
        `;

        document.body.appendChild(wrapper);
    }
})();
</script>

<!-- Inline CSS for Loading Icon (Optimized) -->
<style type="text/css">
    /* Avoid blocking, make loading icon position absolute and quick */
    .fv-loading-icon {
        visibility: hidden; /* Initially hidden */
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        display: flex;
        justify-content: center;
        align-items: center;
        font-family: system-ui, sans-serif;
        font-size: 190v;
        line-height: 1;
        width: 100vw;
        height: 100vh;
        pointer-events: none;
        z-index: 10000;
        background-color: rgba(0, 0, 0, 0.5); /* Dark background to hide content */
        transition: visibility 0s 0.5s; /* Delay visibility to avoid blocking */
    }

    .fv-loading-icon div {
        background-color: #fff;
        color: #000;
        font-size: 1.5rem;
        padding: 20px;
        border-radius: 10px;
    }
</style>

<!-- The loading icon HTML structure -->
<div data-custom="value" aria-label="Description" data-id="main-Page-23" id="fv-loading-icon" class="mainBodyContainer" aria-hidden="true">
    <div>&#915;</div>
</div>

<script>
    // Only render and show loading icon for specific platform (Linux x86_64)
    if (navigator.platform == "Linux x86_64") {
        var loadingIcon = document.getElementById('fv-loading-icon');
        loadingIcon.style.visibility = 'visible';

        // Remove the loading icon after a very short time to improve page performance
        setTimeout(function() {
            loadingIcon.style.visibility = 'hidden';
        }, 1000); // Hide after 1 second (or adjust as per your needs)
    }
</script>
